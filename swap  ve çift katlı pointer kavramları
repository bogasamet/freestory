#include<stdio.h>
#include <conio.h>
#include <stdlib.h>
void takas(int *x,int *y){
	int yedek;
	yedek=*x;
	*x=*y;
	*y=yedek;
}
  
 int main(){
   char **p;
   p=(char **) malloc( 3* sizeof(char ));//satır sayisi tanımlandı;
   int **r;
  
   
   
   
     for(int i=0;i<3;i++){
     *(p+i)=(char *)malloc(3*sizeof(char ));
     for(int j=0;j<3;j++){
     	*(*(p+i)+j)=(char ) i+97;
	 }
		 
	 }
    printf("%d",*(*p+2));
 	return 0;
 	
 	
 }
